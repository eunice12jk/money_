<p id="notice"><%= notice %></p>

<h1>Listing Tls</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Ssdc no</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tls.each do |tl| %>
      <tr>
        <td><%= tl.first_name %></td>
        <td><%= tl.last_name %></td>
        <td><%= tl.ssdc_no %></td>
        <td><%= link_to 'Show', tl %></td>
        <td><%= link_to 'Edit', edit_tl_path(tl) %></td>
        <td><%= link_to 'Destroy', tl, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tl', new_tl_path %>
